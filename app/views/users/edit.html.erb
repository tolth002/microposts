<h1>Profile編集</h1> 
  
<div class="row">
  <div class="col-md-6 col-md-offset-3">
  <%= form_for(@user) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>


      <%= f.submit "編集", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>


